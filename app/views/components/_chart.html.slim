.well.well-chart(id=name)
  .chart-count
  .chart-sum

javascript:
  var labels = #{ counts.keys.map { |date| date.to_date.strftime('%d/%m') }.to_json.html_safe };

  new frappe.Chart("##{name} .chart-count", {
    data: {
      labels: labels,
      datasets: [
        {
          name: "Summe",
          values: #{ counts.values.to_json.html_safe },
        },
      ],
    },
    type: 'line',
    colors: ['#0C478B'],
  });

  new frappe.Chart("##{name} .chart-sum", {
    data: {
      labels: labels,
      datasets: [
        {
          name: "Delta",
          values: #{ sums.values.to_json.html_safe },
        },
      ],
    },
    type: 'bar',
    colors: ['#0C478B'],
  });
