const Appsignal = require('@appsignal/javascript').default;
// For CommonJS module
const appsignal = new Appsignal({
  key: "<%= ENV['APPSIGNAL_WEB_KEY'] || 'none' %>",
  revision: "<%= ENV['RENDER_GIT_COMMIT'] || 'none' %>",
  ignoreErrors: [/UnhandledPromiseRejectionError/],
});
const { plugin } = require('@appsignal/plugin-window-events');

appsignal.use(plugin());
